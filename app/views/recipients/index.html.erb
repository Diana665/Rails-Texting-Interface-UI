<h1>Recipients#index</h1>
<p>Find me in app/views/recipients/index.html.erb</p>


<ul class="recipients">
  <% @recipients.each do |recipient| %>
    <div>
      <tr>
        <td><%= recipient.name %></td>
        <td><%= recipient.email %></td>
        <td><%= recipient.phone %></td>
	  </tr>
    </div>
    <% end %>
</ul>
